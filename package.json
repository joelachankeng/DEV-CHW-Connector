    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "netlify-plugin-cypress": "^2.2.1",
    "npm-run-all2": "^5.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react-use": "^17.5.0",
