    "react-tooltip": "^5.26.3",
    "remix": "^2.9.1",
    "remix-utils": "^7.6.0",
    "slick-carousel": "^1.8.1",
    "string-strip-html": "^13.4.8",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@headlessui/react": "^2.0.4",
    "@kunukn/react-collapse": "^3.0.15",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
